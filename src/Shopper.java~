public class Shopper {
    private String name;
    private int age1;
    private String city;
    public Shopper(String name, int age, String city) {
        this.name = name;
        this.age1 = age1;
        this.city = city;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age1;
    }

    public void setAge(int age1) {
        this.age1 = age1;
    }

    public String getCity() {
        return city;
    }

    public void setCity(String city) {
        this.city = city;
    }
}
